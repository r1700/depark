{
  "name": "depark",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "db:migrate": "npx sequelize-cli db:migrate --migrations-path ./packages/migrations --config ./sequelize.config.js",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo --migrations-path ./packages/migrations --config ./sequelize.config.js",
    "migration:generate": "npx sequelize-cli migration:generate --migrations-path ./packages/migrations --config ./sequelize.config.js",
    "install:all": "npm install --workspaces",
    "start:frontend": "npm start --workspace packages/frontend",
    "start:backend": "npm start --workspace packages/backend",
    "dev:opc": "npm run dev --workspace=packages/opc",
    "dev:backend": "npm run dev --workspace=packages/backend",
    "dev:frontend": "npm run start --workspace=packages/frontend"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@react-oauth/google": "^0.12.2",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/react": "^19.1.9",
    "@types/react-redux": "^7.1.34",
    "dotenv": "^17.2.1",
    "express": "^4.17.1",
    "google-auth-library": "^10.2.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "sequelize-typescript": "^2.1.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.25",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@types/multer": "^2.0.0",
    "@types/pg": "^8.15.5",
    "babel-loader": "^8.2.3",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss-preset-env": "^10.2.4",
    "webpack": "^5.64.4"
  }
}

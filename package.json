{
  "name": "depark",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "db:migrate": "npx sequelize-cli db:migrate --migrations-path ./packages/migrations --config ./sequelize.config.js",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo --migrations-path ./packages/migrations --config ./sequelize.config.js",
    "migration:generate": "npx sequelize-cli migration:generate --migrations-path ./packages/migrations --config ./sequelize.config.js",
    "dev:opc": "npm run dev --workspace=packages/opc",
    "dev:backend": "npm run dev --workspace=packages/backend",
    "dev:frontend": "npm run start --workspace=packages/frontend",
    "build:opc": "npm run build --workspace=packages/opc",
    "build:backend": "npm run build --workspace=packages/backend",
    "build:frontend": "npm run build --workspace=packages/frontend",
    "install:all": "npm install && npm install --workspace=packages/backend && npm install --workspace=packages/frontend && npm install --workspace=packages/opc",
    "start:frontend": "npm start --workspace packages/frontend",
    "start:backend": "npm start --workspace packages/backend"
  },
  

  "dependencies": {
    "express": "^4.17.1",
    "moment": "^2.30.1",
    "pg": "^8.16.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/bcrypt": "^6.0.0",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/moment": "^2.11.29",
    "@types/node": "^24.1.0",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/react-redux": "^7.1.34",
    "@types/winston": "^2.4.4",
    "babel-loader": "^8.2.3",
    "eslint": "^7.32.0",
    "supabase": "^2.33.7",
    "typescript": "^5.8.3",
    "webpack": "^5.64.4"
  }
}
{
  "name": "@depark/backend",
  "version": "1.0.0",
  "description": "Backend API server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "db:migrate": "npx sequelize-cli db:migrate --migrations-path ./migrations --config ./sequelize.config.js",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo --migrations-path ./migrations --config ./sequelize.config.js",
    "migration:generate": "npx sequelize-cli migration:generate --migrations-path ./migrations --config  ./sequelize.config.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.8",
    "@types/sequelize": "^4.28.20",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "google-auth-library": "^10.2.0",
    "joi": "^17.13.3",
    "moment": "^2.30.1",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "react-router-dom": "^7.7.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "sequelize-typescript": "^2.1.6",
    "tsconfig-paths": "^4.2.0",
    "typ": "^0.6.3",
    "typeorm": "^0.3.25",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.23",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^30.0.0",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/moment": "^2.11.29",
    "@types/node": "^20.19.9",
    "@types/pg": "^8.15.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/supertest": "^6.0.3",
    "@types/winston": "^2.4.4",
    "axios": "^1.11.0",
    "jest": "^30.0.4",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "nodemon": "^3.1.10",
    "pg": "^8.16.3",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
